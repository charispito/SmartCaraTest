<UserControl x:Class="SmartCaraTest.controls.ParameterLine1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SmartCaraTest.controls"
             mc:Ignorable="d" 
             d:DesignHeight="187" d:DesignWidth="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Label Content="Mode1" Margin="0, 3" FontWeight="Black"/>
        <local:ParameterLabel Grid.Row="1" Height="17" x:Name="Mode11" Title="ON TIME CW"/>
        <local:ParameterLabel Grid.Row="2" Height="17" x:Name="Mode12" Title="OFF TIME CW" Margin="0, 2, 0, 0"/>
        <local:ParameterLabel Grid.Row="3" Height="17" x:Name="Mode13" Title="ON TIME CCW" Margin="0, 2, 0, 0"/>
        <local:ParameterLabel Grid.Row="4" Height="17" x:Name="Mode14" Title="OFF TIME CCW" Margin="0, 2, 0, 0"/>
        <local:ParameterLabel Grid.Row="5" Height="17" x:Name="Mode15" Title="HEATER TEMP" Margin="0, 2, 0, 0"/>
        <local:ParameterLabel Grid.Row="6" Height="17" x:Name="Mode16" Title="HEATER OFF TIME" Margin="0, 2, 0, 0"/>
        <local:ParameterLabel Grid.Row="7" Height="17" x:Name="Mode17" Title="VENTILE TEMP" Margin="0, 2, 0, 0"/>
        <local:ParameterLabel Grid.Row="8" Height="17" x:Name="Mode18" Title="OPERATE TIME" Margin="0, 2, 0, 0"/>

        <Label Content="Mode2" Grid.Column="1" Margin="0, 3" FontWeight="Black"/>
        <local:ParameterLabel Grid.Row="1" Grid.Column="1" Height="17" x:Name="Mode21" Title="ON TIME CW" Margin="2, 0, 0, 0"/>
        <local:ParameterLabel Grid.Row="2" Grid.Column="1" Height="17" x:Name="Mode22" Title="OFF TIME CW" Margin="2, 2, 0, 0"/>
        <local:ParameterLabel Grid.Row="3" Grid.Column="1" Height="17" x:Name="Mode23" Title="ON TIME CCW" Margin="2, 2, 0, 0"/>
        <local:ParameterLabel Grid.Row="4" Grid.Column="1" Height="17" x:Name="Mode24" Title="OFF TIME CCW" Margin="2, 2, 0, 0"/>
        <local:ParameterLabel Grid.Row="5" Grid.Column="1" Height="17" x:Name="Mode25" Title="HEATER TEMP" Margin="2, 2, 0, 0"/>
        <local:ParameterLabel Grid.Row="6" Grid.Column="1" Height="17" x:Name="Mode26" Title="HEATER OFF TIME" Margin="2, 2, 0, 0"/>
        <local:ParameterLabel Grid.Row="7" Grid.Column="1" Height="17" x:Name="Mode27" Title="VENTILE TEMP" Margin="2, 2, 0, 0"/>
        <local:ParameterLabel Grid.Row="8" Grid.Column="1" Height="17" x:Name="Mode28" Title="OPERATE TIME" Margin="2, 2, 0, 0"/>

        <Label Content="Mode3" Grid.Column="2" Margin="0, 3" FontWeight="Black"/>
        <local:ParameterLabel Grid.Row="1" Grid.Column="2" Height="17" x:Name="Mode31" Title="ON TIME CW" Margin="2, 0, 0, 0"/>
        <local:ParameterLabel Grid.Row="2" Grid.Column="2" Height="17" x:Name="Mode32" Title="OFF TIME CW" Margin="2, 2, 0, 0"/>
        <local:ParameterLabel Grid.Row="3" Grid.Column="2" Height="17" x:Name="Mode33" Title="ON TIME CCW" Margin="2, 2, 0, 0"/>
        <local:ParameterLabel Grid.Row="4" Grid.Column="2" Height="17" x:Name="Mode34" Title="OFF TIME CCW" Margin="2, 2, 0, 0"/>
        <local:ParameterLabel Grid.Row="5" Grid.Column="2" Height="17" x:Name="Mode35" Title="HEATER TEMP" Margin="2, 2, 0, 0"/>
        <local:ParameterLabel Grid.Row="6" Grid.Column="2" Height="17" x:Name="Mode36" Title="HEATER OFF TIME" Margin="2, 2, 0, 0"/>
        <local:ParameterLabel Grid.Row="7" Grid.Column="2" Height="17" x:Name="Mode37" Title="VENTILE TEMP" Margin="2, 2, 0, 0"/>
        <local:ParameterLabel Grid.Row="8" Grid.Column="2" Height="17" x:Name="Mode38" Title="OPERATE TIME" Margin="2, 2, 0, 0"/>
    </Grid>
</UserControl>
