             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SmartCaraTest.controls"
             mc:Ignorable="d" 
             Background="White"
             d:DesignHeight="36" d:DesignWidth="200">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <local:ParameterLabel Grid.Row="0" Height="17" x:Name="Fan1" Title="배기 FAN 대기 모드"/>
        <local:ParameterLabel Grid.Row="1" Height="17" x:Name="Fan2" Title="배기 FAN 운전 모드"/>
    </Grid>
